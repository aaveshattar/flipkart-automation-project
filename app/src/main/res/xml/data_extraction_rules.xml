<?xml version="1.0" encoding="utf-8"?>
<data-extraction-rules>
    <cloud-backup>
        <!-- Include/exclude files for cloud backup -->
        <exclude domain="sharedpref" path="sensitive_prefs.xml"/>
        <exclude domain="database" path="secret.db"/>
    </cloud-backup>
    <device-transfer>
        <!-- Rules for device-to-device transfer -->
        <exclude domain="sharedpref" path="temp_data.xml"/>
    </device-transfer>
</data-extraction-rules>